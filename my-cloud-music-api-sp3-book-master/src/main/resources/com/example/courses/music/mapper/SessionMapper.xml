<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"  >
<mapper namespace="com.example.courses.music.mapper.SessionMapper">
    <insert id="replace">
        replace
        into `session`(
            id,
            platform,
            device,
            `push`,
            `ip`,
            `user_id`
        )values(
        #{id},
        #{platform},
        #{device},
        #{push},
        #{ip},
        #{userId}
        )

    </insert>
</mapper>